<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDisplayName</key>
	<string>Sacavia</string>
	<key>CFBundleIdentifier</key>
	<string>com.sacavia.app</string>
	<key>CFBundleVersion</key>
	<string>1.0</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleName</key>
	<string>Sacavia</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleLocalizations</key>
	<array>
		<string>en</string>
	</array>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchScreen</key>
	<dict>
		<key>UIImageName</key>
		<string>AppIcon</string>
		<key>UIColorName</key>
		<string>AccentColor</string>
		<key>UIImageRespectsSafeAreaInsets</key>
		<true/>
		<key>UILaunchScreenMinimumOSVersion</key>
		<string>17.0</string>
	</dict>
	<key>UIRequiredDeviceCapabilities</key>
	<array>
		<string>armv7</string>
	</array>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
	</array>

	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UIApplicationSceneManifest</key>
	<dict>
		<key>UIApplicationSupportsMultipleScenes</key>
		<true/>
		<key>UISceneConfigurations</key>
		<dict>
			<key>UIWindowSceneSessionRoleApplication</key>
			<array>
				<dict>
					<key>UISceneConfigurationName</key>
					<string>Default Configuration</string>
					<key>UISceneDelegateClassName</key>
					<string>$(PRODUCT_MODULE_NAME).SceneDelegate</string>
				</dict>
			</array>
		</dict>
	</dict>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<false/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>sacavia.com</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<false/>
				<key>NSExceptionMinimumTLSVersion</key>
				<string>TLSv1.2</string>
				<key>NSExceptionRequiresForwardSecrecy</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Sacavia uses your location to show you nearby places, events, and people in your area. This helps you discover amazing local experiences.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Sacavia uses your location to show you nearby places, events, and people in your area. This helps you discover amazing local experiences.</string>
	<key>NSCameraUsageDescription</key>
	<string>Sacavia uses your camera to let you take photos of places and share them with the community.</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Sacavia uses your photo library to let you share photos of places and experiences with the community.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Sacavia uses your microphone to record videos when sharing places and experiences.</string>
	<key>NSUserNotificationUsageDescription</key>
	<string>Sacavia sends notifications about new places, events, and updates from the community to keep you informed about amazing discoveries.</string>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>UIUserInterfaceStyle</key>
	<string>Light</string>
	<key>UIRequiresFullScreen</key>
	<false/>
	<key>UIStatusBarHidden</key>
	<false/>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<true/>
	<key>UIStatusBarStyle</key>
	<string>UIStatusBarStyleDefault</string>
	<key>UIInterfaceOrientation</key>
	<string>UIInterfaceOrientationPortrait</string>


	<key>UIPrerenderedIcon</key>
	<false/>
	<key>UIDeviceFamily</key>
	<array>
		<integer>1</integer>
		<integer>2</integer>
	</array>
	<key>UINativeDisplayRequired</key>
	<true/>
	<key>UILaunchScreenRequiredForNativeResolution</key>
	<true/>
	<key>UIViewControllerBasedStatusBarAppearance</key>
	<true/>


	<key>UIRequiresPersistentWiFi</key>
	<false/>
	<key>UIApplicationExitsOnSuspend</key>
	<false/>
	<key>UILaunchImageMinimumOSVersion</key>
	<string>17.0</string>

	<key>UIStatusBarTintParameters</key>
	<dict>
		<key>UINavigationBar</key>
		<dict>
			<key>Style</key>
			<string>UIBarStyleDefault</string>
			<key>Translucent</key>
			<false/>
		</dict>
	</dict>
	<key>UIApplication Supports iTunes file sharing</key>
	<false/>
	<key>UIFileSharingEnabled</key>
	<false/>
	<key>UIContentSizeCategory</key>
	<string>UIContentSizeCategoryMedium</string>


	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	<key>UIRespondsToUserInteractionInVisionOS</key>
	<true/>
	<key>LSApplicationCategoryType</key>
	<string>public.app-category.social-networking</string>
	<key>CFBundleURLTypes</key>
	<array>
		<dict>
			<key>CFBundleURLName</key>
			<string>com.sacavia.app</string>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>sacavia</string>
			</array>
		</dict>
	</array>
	<key>UIDesignRequiresCompatibility</key>
	<false/>
</dict>
</plist> 